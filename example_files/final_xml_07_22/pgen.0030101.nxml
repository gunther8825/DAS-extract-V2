<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">PLoS Genet</journal-id><journal-id journal-id-type="iso-abbrev">PLoS Genet</journal-id><journal-id journal-id-type="publisher-id">pgen</journal-id><journal-id journal-id-type="publisher-id">plge</journal-id><journal-id journal-id-type="pmc">plosgen</journal-id><journal-title-group><journal-title>PLoS Genetics</journal-title></journal-title-group><issn pub-type="ppub">1553-7390</issn><issn pub-type="epub">1553-7404</issn><publisher><publisher-name>Public Library of Science</publisher-name><publisher-loc>San Francisco, USA</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17571927</article-id><article-id pub-id-type="pmc">1904365</article-id><article-id pub-id-type="doi">10.1371/journal.pgen.0030101</article-id><article-id pub-id-type="publisher-id">07-PLGE-RA-0061R3</article-id><article-id pub-id-type="sici">plge-03-06-13</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group><subj-group subj-group-type="Discipline"><subject>Plant Biology</subject><subject>Plant Biology</subject><subject>Plant Biology</subject></subj-group><subj-group subj-group-type="System Taxonomy"><subject>Zea</subject><subject>Plants</subject><subject>Eukaryotes</subject><subject>None</subject></subj-group></article-categories><title-group><article-title>Laser Microdissection of Narrow Sheath Mutant Maize Uncovers Novel Gene Expression in the Shoot Apical Meristem</article-title><alt-title alt-title-type="running-head">Laser Microdissection of Maize SAM</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Zhang</surname><given-names>Xiaolan</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Madi</surname><given-names>Shahinez</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Borsuk</surname><given-names>Lisa</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Nettleton</surname><given-names>Dan</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Elshire</surname><given-names>Robert J</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Buckner</surname><given-names>Brent</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Janick-Buckner</surname><given-names>Diane</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Beck</surname><given-names>Jon</given-names></name><xref ref-type="aff" rid="aff7">7</xref></contrib><contrib contrib-type="author"><name><surname>Timmermans</surname><given-names>Marja</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Schnable</surname><given-names>Patrick S</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Scanlon</surname><given-names>Michael J</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff5">5</xref><xref ref-type="corresp" rid="cor1">*</xref></contrib></contrib-group><aff id="aff1"><label>1</label> Plant Biology Department, University of Georgia, Athens, Georgia, United States of America
